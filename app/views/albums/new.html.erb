<div class="contents row">
  <div class="container">
    <h3>アルバム作成</h3>
    <%= form_with(model: @album, local: true) do |form| %>
      <%= form.label "アルバム名" %>
      <%= form.text_field :title %>

      <%= form.label "アルバム写真" %>
      <%= form.file_field :image %>
      <%= form.submit "作成する" %>
    <% end %>
  </div>
</div>